import java.util.Scanner;
public class Test2 {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int a;
		int b;
		int cab;
		System.out.print("첫번째 숫자 입력하시오: ");
		a = scan.nextInt();
		
		System.out.print("두번째 숫자 입력하시오: ");
		b = scan.nextInt();
		
		cab = a + b;
		
		System.out.println(cab);
}
